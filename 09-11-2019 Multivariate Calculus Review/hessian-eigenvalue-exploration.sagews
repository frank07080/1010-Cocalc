@interact
def interactive_function(a = slider(0, 10, .05, default = 4),
                         b = slider(-3, 3, .1, default = 1),
                         c = slider(-3, 3, .1, default = 1)):
    f(x,y) = a*x^2 + 2*b*x*y + c*y^2
    show("eigenvalues are " + str(Matrix([[a,b],[b,c]]).eigenvalues()))
    plot3d(f, (x, -5, 5), (y, -5, 5), adaptive = True, color= ['red', 'blue'], opacity = 0.5, aspect_ratio = [1,1,0.1]).show()
